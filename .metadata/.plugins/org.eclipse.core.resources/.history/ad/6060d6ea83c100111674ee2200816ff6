package com.pattern;

import java.util.Vector;

import com.datatype.Point;
import com.primitive2d.Ellipse;
import com.primitive2d.Line;
import com.primitive2d.Polygon;

//base class for all patterns
public class Pattern {
	private Vector<Line> lines = new Vector<Line>();
	private Vector<Point> points = new Vector<Point>(); 
	private Vector<Polygon> polygons = new Vector<Polygon>();
	private Vector<Ellipse> ellipses = new Vector<Ellipse>();

}
